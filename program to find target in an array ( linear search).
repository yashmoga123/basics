#include <bits/stdc++.h>
using namespace std;

int main() {
	
	int n;
	cin>>n;
	int ans = -1;
	
	int arr[n];
	int target;
	
	for( int i = 0; i < n; i++){
		
		cin>>arr[i];
	}
	cin>>target;
	
	for( int i = 0; i < n; i++){
		
		if( arr[i] == target){
			
			ans = i;
		}
	}
	cout<<"  "<<ans;
	
	
	
	return 0;
}
