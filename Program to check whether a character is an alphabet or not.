#include <bits/stdc++.h>
using namespace std;
 
int main() {
 
    char ch;
    cin>>ch;
 
    if( ch >= 'a' && ch <= 'z'){
 
    	cout<<ch<<" is an alphabet ";
    }
    else if( ch >= 'A' && ch <= 'Z'){
 
    	cout<<ch<<" is an alphabet ";
    }
    else {
 
    	cout<<ch<<" is not an alphabet ";
    }
 
 
 
 
	return 0;
}
