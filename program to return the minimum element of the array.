class Solution {
public:
    int findMin(vector<int>& nums) {
        
        int minelement = INT_MAX;
        int n = nums.size();
        for( int i = 0;i < n; i++){
            if( nums[i]<minelement){

                minelement = nums[i];
            }
        }
        return minelement;
    }
};
