#include<bits/stdc++.h>
using namespace std;

int main() {
	
	int a,b,c;
	cin>>a>>b>>c;
	
	if( a+b >= c){
		
		cout<<" it is valid ";
	}
	else if( b+c>= a){
		
		cout<<" it is valid ";
	}
	else if( c+a>= b){
		
		cout<<" it is valid ";
	}
	else {
		
		cout<<" it is not valid ";
	}
	
	
	
	
	return 0;
}
