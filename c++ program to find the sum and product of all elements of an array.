#include<bits/stdc++.h>
using namespace std;

int main() {
    
    int n;
    cin>>n;
    int A[n];
    int sum = 0;
    int product = 1;
    
    for(int i = 0; i < n; i++){
    	
    	cin>>A[i];
    }
    for(int i = 0; i < n; i++ ){
    	
    	cout<<A[i]<<" ";
    }
    cout<<endl;
    for( int i = 0; i < n; i++ ){
    	
    	sum += A[i];
    	product *= A[i];
    }
     cout<<sum<<endl;
     cout<<product<<endl;
    
    
	return 0;
}
