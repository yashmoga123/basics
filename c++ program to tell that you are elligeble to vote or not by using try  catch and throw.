#include <bits/stdc++.h>
using namespace std;

int main() 
{
	try
	{
		int age = 12;
		if(age>=18)
		{
			cout<<" you are elligable to vote. ";
		}
		else
		{
			throw(age);
		}
	}
	catch(int age )
	{
		cout<<" sorry you are not elligible to vote."<<endl;
		cout<<" your age is "<<age;
	}
}	
	
	
	
	
	
	
	
