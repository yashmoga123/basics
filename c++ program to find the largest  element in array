#include <bits/stdc++.h>
using namespace std;

int main() {
	
	int n;
	cin>>n;
	int A[n];
	
	for(int i = 0;i<n;i++){
		
		cin>>A[i];
	}
	for(int i = 0;i<n;i++){
		
		cout<<A[i]<<" ";
	}
	cout<<endl;
	int target;
	cin>>target;
	cout<<" target = "<<target;
	cout<<endl;
	
	for(int i = 0;i<n;i++){
		if(A[i]==target){
			
			cout<<i;
		}
		else{
			cout<<" -1 ";
		}
	}
	
	
	
	
	
	return 0;
}
