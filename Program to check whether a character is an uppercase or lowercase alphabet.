#include <bits/stdc++.h>
using namespace std;
 
int main() {
 
    char ch;
    cin>>ch;
 
    if( ch>='a' && ch<='z'){
 
    	cout<<" it is an lower case ";
    }
    else if(ch>='A' && ch<='Z' ){
 
    	cout<<" it is an uppercase ";
    }
    else {
 
    	cout<<" it is not an alphabet or anything else  ";
    }
 
 
	return 0;
}
