#include<bits/stdc++.h>
using namespace std;
bool isTargetPresent(int A[], int target, int n){
	for(int i=0; i<n; i++){
		if(A[i]==target){
			return true;
		}
        
	}
	return false;
}
int main(){
	
	int n;
	cin>>n;
	
	int A[n];
	for(int i=0; i<n; i++){
		cin>>A[i];
	}
	int target;
	cin>>target;

	isTargetPresent(A,n,target);
	cout<<isTargetPresent(A,n,target);
	return 0;

}
