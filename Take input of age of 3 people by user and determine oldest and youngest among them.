#include <iostream>
using namespace std;

int main() {
	
    int a,b,c;
    cin>>a>>b>>c;
    
    if( a>=b && a>=c){
    	
    	cout<<" a is the oldest "<<endl;
    }
    else{
    	 
    	 cout<<" a is the youngest -----* "<<endl;
    	 
    }
    
     if( b>=a && b>=c){
    	
    	cout<<" b is the oldest-----* ";
    	
    }
    else{
    	
    	cout<<" b is the youngest -----* "<<endl;
    	
    	
    }
     if(c>=a && c>=b){
     	
     	cout<<" c is the oldest-----* "<<endl;
     }
     else{
     	
     	cout<<" c is the youngest-----* "<<endl;
     }
    	
    	
    
	

	
	return 0;
}
