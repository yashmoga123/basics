#include <bits/stdc++.h>
using namespace std;

int main() {
	
	int a,b,max_num;
	
	a = 42;
	b = 63;
	
	max_num= max(a,b);
	
	while(1)
	{
		if( max_num % a == 0 && max_num % b == 0){
			
			cout<<" the lcm of "<<a<<" and "<<b<<" is = "<<max_num<<endl;
			
			  break;
		}
		++max_num;
	}
	
	
	
	return 0;
}
