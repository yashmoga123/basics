#include<bits/stdc++.h>
using namespace std;
 
int main() {
 
	int a,b,c;
	cin>>a>>b>>c;
 
	if( a==b && a==c){
 
		cout<<" it is an equilateral triangle ";
	}
	else if( a == b && a == c && b == c){
 
		cout<<" it is an isoscales triangle ";
	}
	else if( a != b && a != c){
 
		cout<<" it is an scalen triangle ";
	} 
 
 
 
 
 
 
	return 0;
}
