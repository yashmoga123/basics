#include <bits/stdc++.h>
using namespace std;

int main() {
    
    int n;
    cin>>n;
    int A[n];
    
    for(int i = 0;i<n;i++){
    	
    	cin>>A[i];
    }
    for(int i = 0;i<n;i++){
    	
    	cout<<A[i]<<" ";
    }
    cout<<endl;
    for(int i = 0;i<n;i++){
    	
    	if(A[0]<A[i]){
    		
    		A[0]=A[i];
    	}
    	
    		
    	   
    	
    }
    cout<<A[0];
    
    
    
    

	return 0;
}
