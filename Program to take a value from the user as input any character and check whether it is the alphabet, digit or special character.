#include <bits/stdc++.h>
using namespace std;
 
int main() {
 
	char character;
	cin>> character;
 
	if( character>='a' &&  character<='z'){
 
		cout<< character<<" is an alphabet ";
	}
	else if(  character>='A' &&  character<='Z'){
 
		cout<< character<<" is an alphabet ";
	}
	else if(  character>=0){
 
		cout<< character<<" is a digit ";
	}
	else {
 
		cout<< character<<" is a special char  ";
	}
 
 
 
 
	return 0;
}
